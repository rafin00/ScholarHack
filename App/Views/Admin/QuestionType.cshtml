@model IEnumerable<Repo.QuestionType>

@{
    ViewBag.Title = "QuestionType";
}

<h2>QuestionType</h2>

<p>
    <form method="post">
        <input name="QuestionTypeName" /> <input type="submit" value="Add" />
    </form>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.QuestionTypeName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.QuestionTypeName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.QuestionTypeID }) |
            @Html.ActionLink("Details", "Details", new { id=item.QuestionTypeID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.QuestionTypeID })
        </td>
    </tr>
}

</table>
