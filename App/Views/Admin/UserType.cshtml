@model IEnumerable<Repo.UserType>

@{
    ViewBag.Title = "UserType";
}

<h2>UserType</h2>

<p>
    <form method="post">
        <input name="UserTypeName" /> <input type="submit" value="Add" />
    </form>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserTypeName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserTypeName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.UserTypeID }) |
            @Html.ActionLink("Details", "Details", new { id=item.UserTypeID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.UserTypeID })
        </td>
    </tr>
}

</table>
