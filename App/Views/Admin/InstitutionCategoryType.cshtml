@model IEnumerable<Repo.InstitutionCategoryType>

@{
    ViewBag.Title = "InstitutionCategoryType";
}

<h2>InstitutionCategoryType</h2>

<p>
    <form method="post">
        <input name="InstitutionCategoryTypeName" /> <input type="submit" value="Add" />
    </form>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.InstitutionCategoryTypeName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.InstitutionCategoryTypeName)
        </td>
        <td>
            @Html.ActionLink("Classes", "InstitutionCategoryClass", new { InstitutionCategoryTypeID=item.InstitutionCategoryTypeID }) |
            @Html.ActionLink("Details", "Details", new { id=item.InstitutionCategoryTypeID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.InstitutionCategoryTypeID })
        </td>
    </tr>
}

</table>
